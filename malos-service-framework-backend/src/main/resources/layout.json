{
  "LEVEL": {
    "$resolver": "level",
    "field": "name"
  },
  "APPLICATION": "${spring:spring.application.name}",
  "AWS-TRACE-UUID": {
    "$resolver": "mdc",
    "stringified": true,
    "key": "AWS-TRACE-UUID"
  },
  "MESSAGE": {
    "CONTENTS": {
      "$resolver": "message",
      "stringified": true
    },
    "STACKTRACE":{
      "$resolver": "exception",
      "field": "stackTrace",
      "stackTrace": {
        "stringified": true
      }
    }
  }
}